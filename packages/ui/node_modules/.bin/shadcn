#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/l/Remix-Proj/CreditManager/node_modules/.pnpm/shadcn@3.6.1_@types+node@22.19.2_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/dist/node_modules:/mnt/l/Remix-Proj/CreditManager/node_modules/.pnpm/shadcn@3.6.1_@types+node@22.19.2_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/node_modules:/mnt/l/Remix-Proj/CreditManager/node_modules/.pnpm/shadcn@3.6.1_@types+node@22.19.2_hono@4.11.1_typescript@5.9.3/node_modules:/mnt/l/Remix-Proj/CreditManager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/l/Remix-Proj/CreditManager/node_modules/.pnpm/shadcn@3.6.1_@types+node@22.19.2_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/dist/node_modules:/mnt/l/Remix-Proj/CreditManager/node_modules/.pnpm/shadcn@3.6.1_@types+node@22.19.2_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/node_modules:/mnt/l/Remix-Proj/CreditManager/node_modules/.pnpm/shadcn@3.6.1_@types+node@22.19.2_hono@4.11.1_typescript@5.9.3/node_modules:/mnt/l/Remix-Proj/CreditManager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/shadcn@3.6.1_@types+node@22.19.2_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/dist/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/shadcn@3.6.1_@types+node@22.19.2_hono@4.11.1_typescript@5.9.3/node_modules/shadcn/dist/index.js" "$@"
fi
